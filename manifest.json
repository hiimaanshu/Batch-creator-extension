{
    "name": "Batch Creator",
    "version": "0.1",
    "manifest_version": 3,
    "description": "Help in creating a RD batch.",
    "background": {
        "service_worker": "js/service-worker.js"
    },
    "action": {
        "default_title": "Click to open panel"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "128": "images/Logo.png"
    },
    "side_panel": {
        "default_path": "index.html"
    },
    "permissions": [
        "tabs",
        "sidePanel",
        "contextMenus",
        "storage"
    ]
}